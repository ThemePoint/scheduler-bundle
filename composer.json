{
  "name": "flexic/scheduler-bundle",
  "type": "symfony-bundle",
  "description": "Provides an scheduler bundle for symfony framework.",
  "keywords": [
    "symfony",
    "sheduler"
  ],
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Hendrik Legge",
      "email": "info@themepoint.de",
      "homepage": "https://themepoint.de"
    }
  ],
  "require": {
    "php": "^8.1",
    "flexic/scheduler": "^2.0.2",
    "symfony/dependency-injection": "^6.1.5",
    "symfony/http-kernel": "^6.1.5"
  },
  "require-dev": {
    "ergebnis/php-cs-fixer-config": "^4.9.0",
    "phpstan/phpstan": "^1.8.8",
    "phpstan/phpstan-deprecation-rules": "^1.0.0",
    "phpstan/phpstan-strict-rules": "^1.4.4"
  },
  "config": {
    "platform": {
      "php": "8.1"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Flexic\\SchedulerBundle\\": "src/"
    }
  }
}
